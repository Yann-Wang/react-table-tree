{
  "env": {
    "browser": true
  },
  "extends": ["standard", "standard-react"],
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "rules": {
    "jsx-quotes": [2, "prefer-double"],
    "space-before-function-paren": [0],
    "prettier/prettier": "error",
    "indent": [0]
  },
  "globals": {
    "_": false
  }
}
